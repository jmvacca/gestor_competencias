{% extends 'base.html.twig' %}

{% block title %}Ver Lugar De Realizacion{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <div class="col-8">
    <h3>Ver Lugar De Realizacion</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-10">
    <table class="table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lugar_de_realizacion.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ lugar_de_realizacion.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
