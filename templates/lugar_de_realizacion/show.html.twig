        </tbody>
        </table>
        </div>
        <div class="col-2">
        <div class="row">
                <button   type="button" class="btn btn-warning btn-lg btn-block" onclick=location.href="{{ path('lugar_de_realizacion_edit', {'id': lugar_de_realizacion.id}) }}">Editar</button>
        </div>
            <br>
        <div class="row">
                <button type="button" class="btn btn-danger btn-lg btn-block" id="" >Eliminar</button>
