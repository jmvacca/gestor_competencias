    <div class="row">
        <div class="col-3">
        </div>
        <div class="col-6">
            <button type="button" class="btn btn-info btn-lg btn-block" id="" onclick=location.href="{{ path('lugar_de_realizacion_index') }}">Volver a Lugares De Realizacion</button>
        </div>

    </div>


</div>

